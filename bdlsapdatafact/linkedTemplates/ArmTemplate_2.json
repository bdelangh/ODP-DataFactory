			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "TransactionsFromBlobToSQL",
						"type": "Copy",
						"dependsOn": [],
