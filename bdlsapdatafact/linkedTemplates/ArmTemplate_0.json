					"url": "[parameters('S4HOData_properties_typeProperties_url')]",
					"authenticationType": "Basic",
					"userName": "[parameters('S4HOData_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('S4HOData_password')]"
					}
				}
			},
			"dependsOn": []
