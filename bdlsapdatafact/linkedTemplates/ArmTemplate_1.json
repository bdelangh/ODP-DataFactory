			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "CosmosDb",
				"typeProperties": {
					"connectionString": "[parameters('myMicroHackCosmosDB_connectionString')]"
				}
			},
			"dependsOn": []
