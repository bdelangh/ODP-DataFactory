{
	"name": "S4DVBAPDataLake",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzDataLakeGen2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fileId": {
				"type": "string",
				"defaultValue": "fileId"
			}
		},
		"folder": {
			"name": "S4D_MicroHack"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat('vbap_',dataset().fileId,'.csv')",
					"type": "Expression"
				},
				"folderPath": "S4D/VBAP",
				"fileSystem": "sap"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}